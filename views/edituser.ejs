<html>
    <head>
        <title>Edit User</title>
    </head>
    <body>
        <form action="/edit/<%= editUser._id%>?_method=PUT" method="POST">
            <label for="uname">Username</label>
            <input type="text" name="uname" id="uname" value="<%= editUser.username%>"><br>
            <label for="email">Email</label>
            <input type="email" name="email" id="email" value="<%= editUser.email%>" disabled><br>
            <!--<label for="password">Password</label>
            <input type="password" name="password" id="password"><br>-->
            <button>Submit</button>
    </body>
</html>